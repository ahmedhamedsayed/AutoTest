<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC
        "-//Hibernate/Hibernate Configuration DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
    <session-factory>
        <property name="hibernate.connection.driver_class">org.sqlite.JDBC</property>
        <property name="hibernate.connection.username"></property>
        <property name="hibernate.connection.password"></property>
        <property name="hibernate.dialect">configuration.databaseConfiguration.SQLiteDialect</property>
        <property name="hibernate.hbm2ddl.auto">update</property>
        <mapping class="models.Unit"/>
        <mapping class="models.Exam"/>
        <mapping class="models.ExamPercentage"/>
        <mapping class="models.Question"/>
        <mapping class="models.Choose"/>
        <mapping class="models.Choice"/>
        <mapping class="models.Complete"/>
        <mapping class="models.Connect"/>
        <mapping class="models.ConnectPair"/>
        <mapping class="models.MathProblem"/>
        <mapping class="models.Law"/>
        <mapping class="models.LawAnswer"/>
        <mapping class="models.ModelProblem"/>
        <mapping class="models.ModelProblemEntry"/>
        <mapping class="models.ModelProblemAnswer"/>
        <mapping class="models.TrueOrFalse"/>
    </session-factory>
</hibernate-configuration>